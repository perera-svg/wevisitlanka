{
  "name": "wevisitlanka",
  "private": true,
  "type": "module",
  "imports": {
    "#/*": "./src/*"
  },
  "scripts": {
    "dev": "dotenv -e .env.local -- cross-env NODE_OPTIONS=--import=./instrument.server.mjs vite dev --port 3000",
    "dev:ci": "cross-env NODE_OPTIONS=--import=./instrument.server.mjs vite dev --host 127.0.0.1 --port 3000 --strictPort",
    "build": "vite build && cp instrument.server.mjs dist/server",
    "preview": "vite preview",
    "test": "vitest run",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:report": "playwright show-report",
    "deploy": "bun run build && wrangler deploy",
    "format": "biome format",
    "lint": "biome lint",
    "check": "biome check",
    "prepare": "husky",
    "start": "node --import ./dist/server/instrument.server.mjs dist/server/index.mjs"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css,md}": "biome check --no-errors-on-unmatched"
  },
  "dependencies": {
    "@base-ui/react": "^1.2.0",
    "@cloudflare/vite-plugin": "^1.13.8",
    "@fontsource-variable/geist": "^5.2.8",
    "@sentry/tanstackstart-react": "^10.34.0",
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-form": "^1.28.3",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "@workos-inc/authkit-react": "^0.16.0",
    "@workos-inc/node": "^8.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv-cli": "^11.0.0",
    "lucide-react": "^0.545.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "shadcn": "^3.8.5",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.1.18",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@playwright/test": "^1.56.1",
    "@tanstack/devtools-vite": "^0.3.11",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.1.6",
    "typescript": "^5.7.2",
    "vite": "^7.1.7",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5",
    "wrangler": "^4.40.3"
  }
}
